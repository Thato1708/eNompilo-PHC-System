@using eNompilo.v3._0._1.Constants;
@model eNompilo.v3._0._1.Models.Vaccination.VaccinationInventory

@{
    ViewData["Title"] = "Vax Inventory";
    @*must use the dashboard layout*@
}

<form method="post">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>

    <div class="form-group mb-4">
        <label asp-for="Disease" class="control-label"></label>
        <input asp-for="Disease" type="number" class="form-control" placeholder="Enter ID Number" autocomplete="username" aria-required="true" />
        <span asp-validation-for="Disease" class="text-danger"></span>
    </div>


</form>
